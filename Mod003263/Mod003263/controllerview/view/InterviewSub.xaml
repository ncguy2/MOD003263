<UserControl x:Class="Mod003263.controllerview.view.InterviewSub"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Mod003263.controllerview.view"
             xmlns:controls="clr-namespace:Mod003263.wpf.controls"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" Width="965.589" Height="338">
    <Grid Background="#FF5D5D5D">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="131*"/>
            <ColumnDefinition Width="287*"/>
            <ColumnDefinition Width="548*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="25*"/>
            <RowDefinition Height="144*"/>
        </Grid.RowDefinitions>
        <ListView x:Name="listView" Margin="10" Grid.Row="1" Grid.ColumnSpan="2" d:LayoutOverrides="LeftMargin, RightMargin, TopMargin, BottomMargin">
            <ListBox.ItemTemplate>
                <DataTemplate>
                    <StackPanel Orientation="Vertical">
                        <ContentControl>
                            <controls:ApplicantRow/>
                        </ContentControl>
                    </StackPanel>
                </DataTemplate>
            </ListBox.ItemTemplate>
        </ListView>
        <TextBox x:Name="textBox" Margin="10" TextWrapping="Wrap" Grid.Column="1" d:LayoutOverrides="LeftMargin, RightMargin, TopMargin, BottomMargin"/>
        <Label x:Name="label" Content="Applicant Name" Margin="10" FontSize="14" d:LayoutOverrides="LeftMargin, RightMargin, TopMargin, BottomMargin"/>
        <Grid Grid.Column="2" Grid.RowSpan="2" Margin="0,3.893,0,-3.893">
            <Grid.RowDefinitions>
                <RowDefinition Height="42*"/>
                <RowDefinition Height="42*"/>
                <RowDefinition Height="42*"/>
                <RowDefinition Height="204*"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="134*"/>
                <ColumnDefinition Width="107*"/>
                <ColumnDefinition Width="307*"/>
            </Grid.ColumnDefinitions>
            <Border BorderBrush="Black" BorderThickness="1" Margin="7.971,10,5.972,4.214" d:LayoutOverrides="LeftMargin, RightMargin, TopMargin, BottomMargin" Grid.RowSpan="3"/>
            <Label x:Name="label1" Content="Full Name" FontSize="14" Grid.Column="1" d:LayoutOverrides="LeftMargin, RightMargin"/>
            <TextBox x:Name="textBox1" Margin="10,10,10,2.509" TextWrapping="Wrap" FontSize="14" IsReadOnly="True" Grid.Column="2" d:LayoutOverrides="LeftMargin, RightMargin"/>
            <Label x:Name="label2" Content="Email Address" Grid.Row="1" FontSize="14" Grid.Column="1" d:LayoutOverrides="LeftMargin, RightMargin"/>
            <TextBox x:Name="textBox2" Margin="10,10,10,11.107" TextWrapping="Wrap" FontSize="14" IsReadOnly="True" Grid.Column="2" Grid.Row="1" d:LayoutOverrides="LeftMargin, RightMargin, TopMargin, BottomMargin" Height="30"/>
            <TextBox x:Name="textBox3" Grid.Column="2" Margin="10,6.893,10,4.214" Grid.Row="2" TextWrapping="Wrap" FontSize="14" d:LayoutOverrides="LeftMargin, RightMargin, TopMargin, BottomMargin"/>
            <Label x:Name="label3" Content="Phone Number" Grid.Column="1" Grid.Row="2" FontSize="14"/>
        </Grid>
    </Grid>
</UserControl>
